package practise;
public class Excepts {
  public static void main(String[] args) {
	 int a=10, b=0, c;
    try {
      c= a/b;
      System.out.println(b);
    } catch (Exception e) {
    	b= 1;
    	try
    	{
    		c=a/b;
    	}
    	finally
    	{
    	System.out.println("Issue resolved");
    	}
    	
    }
    
  }
}
